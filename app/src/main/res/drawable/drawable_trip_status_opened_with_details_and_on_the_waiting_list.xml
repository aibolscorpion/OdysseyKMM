<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/leftDrawable" android:right="22dp" >
        <shape android:shape="rectangle" >
            <corners android:topLeftRadius="16dp"
                android:bottomLeftRadius="16dp" />
            <solid android:color="#ffbe6b"/>
        </shape>
    </item>
    <item android:id="@+id/rightDrawable"
        android:left="22dp">
        <shape android:shape="rectangle" >
            <corners android:topRightRadius="16dp"
                android:bottomRightRadius="16dp" />
            <solid android:color="#c5cad1"/>
        </shape>
    </item>
    <item android:drawable="@drawable/icons_tabs_train" android:left="20dp"/>
    <item android:drawable="@drawable/icons_tabs_train" android:right="20dp"/>

</layer-list>