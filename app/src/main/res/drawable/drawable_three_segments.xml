<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@+id/firstIconBg"
        android:width="26dp"
        android:height="32dp"
        android:gravity="start"
        android:left="0dp"
        android:right="52dp">
        <shape android:shape="rectangle" >
            <corners android:topLeftRadius="16dp"
                android:bottomLeftRadius="16dp" />
        </shape>
    </item>

    <item android:id="@+id/secondIconBg"
        android:width="26dp"
        android:height="32dp"
        android:gravity="center"
        android:left="26dp"
        android:right="26dp">
        <shape android:shape="rectangle">
        </shape>
    </item>

    <item android:id="@+id/thirdIconBg"
        android:width="26dp"
        android:height="32dp"
        android:gravity="start"
        android:left="52dp">
        <shape android:shape="rectangle" >
            <corners android:topRightRadius="16dp"
                android:bottomRightRadius="16dp" />
        </shape>
    </item>

    <item android:id="@+id/firstIcon"
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/icons_tabs_train"
        android:left="1dp"
        android:gravity="center_vertical"/>

    <item
        android:id="@+id/secondIcon"
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/icons_tabs_train"
        android:left="27dp"
        android:gravity="center_vertical"/>

    <item
        android:id="@+id/thirdIcon"
        android:width="24dp"
        android:height="24dp"
        android:drawable="@drawable/icons_tabs_train"
        android:left="53dp"
        android:gravity="center_vertical"/>
</layer-list>