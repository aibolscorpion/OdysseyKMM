        android:layout="@layout/dialog_id"
        tools:layout="@layout/dialog_id" >
        <argument
            android:name="employee"
            app:argType="kz.divtech.odyssey.shared.domain.model.profile.Profile"
            app:nullable="true" />
        <argument
            android:name="document"
            app:argType="kz.divtech.odyssey.shared.domain.model.profile.Document" />
    </dialog>
    <fragment
