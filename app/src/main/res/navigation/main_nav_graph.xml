        android:id="@+id/action_global_tripDetailDialog"
        app:destination="@id/tripDetailDialog" />
    <action
        android:id="@+id/action_global_ticketsAreNotPurchasedDialog"
        app:destination="@id/ticketsAreNotPurchasedDialog" />
