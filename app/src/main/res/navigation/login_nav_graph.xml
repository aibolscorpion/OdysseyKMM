<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_nav_graph"
    app:startDestination="@id/phoneNumberFragment">

    <fragment
        android:id="@+id/phoneNumberFragment"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.send_sms.SendSmsFragment"
        app:layout="@layout/fragment_send_sms"
        tools:layout="@layout/fragment_send_sms">
        <action
            android:id="@+id/action_phoneNumberFragment_to_termsOfAgreementDialog"
            app:destination="@id/termsOfAgreementDialog" />
        <action
            android:id="@+id/action_phoneNumberFragment_to_accountDeactivatedDialog"
            app:destination="@id/accountDeactivatedDialog" />
        <action
            android:id="@+id/action_phoneNumberFragment_to_codeFragment"
            app:destination="@id/codeFragment" />
        <action
            android:id="@+id/action_phoneNumberFragment_to_phoneNumberErrorDialog"
            app:destination="@id/phoneNumberErrorDialog" />
        <action
            android:id="@+id/action_phoneNumberFragment_to_IINFragment"
            app:destination="@id/IINFragment" />
    </fragment>


    <dialog
        android:id="@+id/termsOfAgreementDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.send_sms.dialogs.TermsOfAgreementDialog"
        android:label="TermsOfAgreementDialog"
        app:layout="@layout/dialog_terms_of_agreement"
        tools:layout="@layout/dialog_terms_of_agreement"/>

    <dialog
        android:id="@+id/phoneNumberErrorDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.send_sms.dialogs.PhoneNumberErrorDialog"
        android:label="PhoneNumberErrorDialog"
        app:layout="@layout/dialog_phone_number_error"
        tools:layout="@layout/dialog_phone_number_error"/>

    <dialog
        android:id="@+id/contactSupportDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.send_sms.dialogs.ContactSupportDialog"
        android:label="ContactSupportDialog"
        app:layout="@layout/dialog_contact_support"
        tools:layout="@layout/dialog_contact_support" />

    <dialog
        android:id="@+id/accountDeactivatedDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.send_sms.dialogs.AccountDeactivatedDialog"
        android:label="AccountDeactivatedDialog"
        app:layout="@layout/dialog_account_deactivated"
        tools:layout="@layout/dialog_account_deactivated" />

    <fragment
        android:id="@+id/codeFragment"
        android:name="kz.divtech.odyssey.rotation.ui.login.auth.fill_code.FillCodeFragment"
        app:layout="@layout/fragment_fill_code"
        tools:layout="@layout/fragment_fill_code" >
        <argument
            android:name="phone_number"
            app:argType="string" />
    </fragment>

    <fragment
        android:id="@+id/IINFragment"
        android:name="kz.divtech.odyssey.rotation.ui.login.search_by_iin.SearchIINFragment"
        android:label="IINFragment"
        app:layout="@layout/fragment_search_by_iin"
        tools:layout="@layout/fragment_search_by_iin" >
        <action
            android:id="@+id/action_IINFragment_to_employeeNotFoundDialog"
            app:destination="@id/employeeNotFoundDialog" />
        <action
            android:id="@+id/action_IINFragment_to_changeNumberFragment"
            app:destination="@id/changeNumberFragment" />
        <action
            android:id="@+id/action_IINFragment_to_addPhoneNumber"
            app:destination="@id/addPhoneNumber" />
        <argument
            android:name="phoneNumber"
            app:argType="string" />
    </fragment>

    <dialog
        android:id="@+id/employeeNotFoundDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.search_by_iin.dialogs.EmployeeNotFoundDialog"
        android:label="EmployeeNotFoundDialog"
        app:layout="@layout/dialog_employee_not_found"
        tools:layout="@layout/dialog_employee_not_found" >
        <argument
            android:name="iin"
            app:argType="string" />
    </dialog>

    <fragment
        android:id="@+id/changeNumberFragment"
        android:name="kz.divtech.odyssey.rotation.ui.login.change_phone.ChangePhoneNumberFragment"
        android:label="ChangeNumberFragment"
        app:layout="@layout/fragment_change_number"
        tools:layout="@layout/fragment_change_number">
        <action
            android:id="@+id/action_changeNumberFragment_to_applicationSentDialog"
            app:destination="@id/applicationSentDialog" />
        <action
            android:id="@+id/action_changeNumberFragment_to_phoneNumberAddedDialog"
            app:destination="@id/phoneNumberAddedDialog" />
        <action
            android:id="@+id/action_changeNumberFragment_to_changeNumberErrorDialog"
            app:destination="@id/changeNumberErrorDialog" />
        <argument
            android:name="employee"
            app:argType="kz.divtech.odyssey.rotation.domain.model.login.login.Employee" />
    </fragment>

    <activity
        android:id="@+id/mainActivity"
        android:name="kz.divtech.odyssey.rotation.ui.MainActivity"
        android:label="activity_main"
        tools:layout="@layout/activity_main" />

    <dialog
        android:id="@+id/applicationSentDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.change_phone.dialogs.ApplicationSentDialog"
        android:label="ApplicationSentDialog"
        app:layout="@layout/dialog_application_sent"
        tools:layout="@layout/dialog_application_sent" />

    <dialog
        android:id="@+id/phoneNumberAddedDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.change_phone.dialogs.PhoneNumberAddedDialog"
        android:label="PhoneNumberAddedDialog"
        app:layout="@layout/dialog_phone_number_added"
        tools:layout="@layout/dialog_phone_number_added">
        <action
            android:id="@+id/action_phoneNumberAddedDialog_to_phoneNumberFragment"
            app:destination="@id/phoneNumberFragment" />
    </dialog>

    <dialog
        android:id="@+id/changeNumberErrorDialog"
        android:name="kz.divtech.odyssey.rotation.ui.login.change_phone.dialogs.ChangeNumberErrorDialog"
        android:label="ChangeNumberErrorDialog"
        app:layout="@layout/dialog_change_number_error"
        tools:layout="@layout/dialog_change_number_error" />
    <fragment
        android:id="@+id/addPhoneNumber"
        android:name="kz.divtech.odyssey.rotation.ui.login.add_phone.AddPhoneNumberFragment"
        android:label="AddPhoneNumber"
        app:layout="@layout/fragment_add_phone"
        tools:layout="@layout/fragment_add_phone">
        <argument
            android:name="employee"
            app:argType="kz.divtech.odyssey.rotation.domain.model.login.login.Employee" />
    </fragment>
    <action
        android:id="@+id/action_global_mainActivity"
        app:destination="@id/mainActivity" />
    <action
        android:id="@+id/action_global_contactSupportDialog"
        app:destination="@id/contactSupportDialog" />

</navigation>